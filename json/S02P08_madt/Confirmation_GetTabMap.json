{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "ITxPT S02P08v2.1.0 MADT service - MADT GetTabMap response JSON Schema Definition",
    "description": "ITxPT Service Interface for Real-time Information relating to Public Transport Operations. - JSON Schema with explicit functional services",
    "default": {},
    "additionalProperties": true,
    "required": [
        "retCode",
        "maxTabs",
        "tabMap"
    ],
    "properties": {
        "retCode": {
            "$id": "#/properties/retCode",
            "type": "integer",
            "enum": [0, 1, 2, 3, 4]
        },
        "maxTabs": {
            "$id": "#/properties/maxTabs",
            "type": "integer"
        },
        "tabMap": {
            "$id": "#/properties/tabMap",
            "type": "array",
            "additionalItems": true,
            "items": {
                "$id": "#/properties/tabMap/items",
                "type": "integer"
            }
        }
    }
}