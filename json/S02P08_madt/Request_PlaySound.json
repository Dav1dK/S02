{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "ITxPT S02P08v2.1.1 MADT service - MADT PlaySound request JSON Schema Definition",
    "description": "ITxPT Service Interface for Real-time Information relating to Public Transport Operations. - JSON Schema with explicit functional services",
    "default": {},
    "additionalProperties": true,
    "required": [
        "req",
        "soundFlags",
        "soundId"
    ],
    "properties": {
        "req": {
            "$id": "#/properties/req",
            "type": "string",
            "const": "PlaySound"
        },
        "soundFlags": {
            "$id": "#/properties/soundFlags",
            "type": "string",
            "enum": ["00001", "00002", "00010", "10000"]
        },  
        "soundId": {
            "$id": "#/properties/soundId",
            "type": "string",
            "examples": [
                "SystemAsterisk",
                "SystemExclamation",
                "SystemExit",
                "SystemHand",
                "SystemNotification",
                "SystemQuestion",
                "SystemStart"
            ]
        }
    }
}