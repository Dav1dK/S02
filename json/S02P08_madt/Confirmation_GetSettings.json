{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "ITxPT S02P08v2.1.0 MADT service - MADT GetSettings response JSON Schema Definition",
    "description": "ITxPT Service Interface for Real-time Information relating to Public Transport Operations. - JSON Schema with explicit functional services",
    "default": {},
    "additionalProperties": true,
    "required": [
        "retCode",
        "volume",
        "brightness",
        "contrast",
        "language"
    ],
    "properties": {
        "retCode": {
            "$id": "#/properties/retCode",
            "type": "integer",
            "enum": [0, 1, 2, 3, 4]
        },
        "volume": {
            "$id": "#/properties/volume",
            "type": "string"
        },
        "brightness": {
            "$id": "#/properties/brightness",
            "type": "integer",
            "minimum": 0,
            "maximum": 255
        },
        "contrast": {
            "$id": "#/properties/contrast",
            "type": "integer",
            "minimum": 0,
            "maximum": 255
        },
        "language": {
            "$id": "#/properties/language",
            "type": "string",
            "description": "alpha2 code according to ISO 639-1",
            "examples": [
                "en"
            ]
        }    }
}